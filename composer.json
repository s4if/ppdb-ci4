{
    "name": "s4if/ppdb-ci4",
    "type": "project",
    "description": "Rewrite Program PPDB dengan CI4",
    "homepage": "https://www.saifulh.web.id",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "codeigniter4/framework": "^4",
        "doctrine/orm": "^2.12",
        "gregwar/captcha": "^1.1",
        "mikehaertl/phpwkhtmltopdf": "^2.5",
        "phpoffice/phpspreadsheet": "^1.23",
        "vlucas/phpdotenv": "^5.4",
        "doctrine/cache": "^2.2",
        "symfony/cache": "^5.4",
        "doctrine/annotations": "^1.13"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "suggest": {
        "ext-fileinfo": "Improves mime type detection for files"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Config\\": "app/Config",
            "Entities\\": "app/Models/Entities"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
